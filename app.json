{
  "expo": {
    "name": "ZanaAI WebView App",
    "slug": "zanaai-webview-app",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo1.png",
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flkrdstudio.zanaaiwebview"
    },
    "android": {
      "package": "com.flkrdstudio.zanaaiwebview",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo1.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/logo1.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "light"
  }
}
